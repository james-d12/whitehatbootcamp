<h1 class="display-2 text-center"> Memes </h1>

{{#each memes }}
    <div class="card" style="width: 18rem;">
        <img class="card-img-top" src={{this.url}} alt="Card image cap">
        <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <p class="card-text"></p>
        </div>
    </div>

{{/each}}
